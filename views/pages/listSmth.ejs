<!DOCTYPE html>
<html>
    <head>
        <% include ../partials/head.ejs %>
        <title>List of something</title>
        <script src="javascript/listSmth.js"></script>
    </head>
    <body>
        <div class="container pt-5">
            <div class="row">
                    <% localStorage.carList.forEach((car) => { %>
                        <div class="col-md-4 mb-5">
                            <div class="card" style="width: 18rem;">
                                <div class="card-body">
                                    <h5 class="card-title"><%= car.name %></h5>
                                    <p class="card-text">Price: <%= car.price %> </p>
                                    <button idOfThis="224" whichCar="<%= car.name %>" class="btn btn-primary" onclick="onBuyCar(this)">Buy car</button>
                                </div>
                            </div>
                        </div>
                    <% }) %>
            </div>
        </div>
    </body>
</html>